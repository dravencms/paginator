{
  "name": "dravencms/paginator",
  "type": "dravencms-package",
  "description": "DravenCMS Paginator (visualPaginator)",
  "keywords": [
    "dravencms",
    "paginator"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "ipub/visual-paginator": "^v1.0.6",
    "dravencms/translation": "^v1.0"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "visualPaginator": "IPub\\VisualPaginator\\DI\\VisualPaginatorExtension",
          "paginator": "Dravencms\\Paginator\\DI\\PaginatorExtension"
        },
        "translation": {
          "dirs": [
            "%appDir%/../vendor/dravencms/paginator/src/Dravencms/Paginator/lang"
          ]
        }
      }
    }
  }
}
